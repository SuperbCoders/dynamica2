.pull-left
  %ul.filter-btn-list{"ng-if" => "1 < pages.length || !autoHide"}
    -#%li{"ng-class" => "{ disabled : pagination.current == 1 }", "ng-if" => "boundaryLinks"}
    -#  %a.btn.btn_v1.check_text{:href => "", "ng-click" => "setCurrent(1)"}
    -#    %span.arr_span ←
    -#    %span &nbsp;Back
    %li{"ng-class" => "{ disabled : pagination.current == 1 }", "ng-if" => "directionLinks"}
      %a.btn.btn_v1.check_text{:href => "", "ng-click" => "setCurrent(pagination.current - 1)"}
        %span.arr_span ←
        %span &nbsp;Back
    -#%li{"ng-class" => "{ disabled : pagination.current == pagination.last }", "ng-if" => "boundaryLinks"}
    -#  %a.btn.btn_v1.check_text{:href => "", "ng-click" => "setCurrent(pagination.last)"}
    -#    %span Next&nbsp;
    -#    %span.arr_span →
    %li{"ng-class" => "{ disabled : pagination.current == pagination.last }", "ng-if" => "directionLinks"}
      %a.btn.btn_v1.check_text{:href => "", "ng-click" => "setCurrent(pagination.current + 1)"}
        %span Next&nbsp;
        %span.arr_span →

    %li{"ng-class" => "{ active : pagination.current == pageNumber, disabled : pageNumber == '...' }", "ng-repeat" => "pageNumber in pages track by tracker(pageNumber, $index)"}
      %a.btn.btn_v2.pagination_btn{:href => "", "ng-click" => "setCurrent(pageNumber)"} {{ pageNumber }}


